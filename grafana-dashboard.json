{
  "title": "Food Delivery CI/CD Monitoring",
  "uid": "food-delivery-cicd",
  "tags": ["ci-cd", "github-actions", "food-delivery"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "timepicker": {},
  "panels": [
      {
        "id": 1,
        "title": "Build Status",
        "type": "stat",
        "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "build_status{job=\"github-actions\"}",
            "legendFormat": "Status",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              { "type": "value", "value": "1", "text": "SUCCESS", "color": "green" },
              { "type": "value", "value": "0", "text": "FAILED", "color": "red" }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "red" },
                { "value": 1, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Test Status",
        "type": "stat",
        "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "test_status{job=\"github-actions\"}",
            "legendFormat": "Status",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "none"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              { "type": "value", "value": "1", "text": "PASSED", "color": "green" },
              { "type": "value", "value": "0", "text": "FAILED", "color": "red" }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "red" },
                { "value": 1, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Total Tests Passed",
        "type": "stat",
        "gridPos": { "x": 12, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "test_status{job=\"github-actions\"} == 1",
            "legendFormat": "Tests",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["count"]
          },
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Deployment Success Rate",
        "type": "gauge",
        "gridPos": { "x": 18, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "sum(build_status{job=\"github-actions\"} == 1) / count(build_status{job=\"github-actions\"}) * 100",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "red" },
                { "value": 70, "color": "yellow" },
                { "value": 90, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Build Duration",
        "type": "timeseries",
        "gridPos": { "x": 0, "y": 4, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "build_duration_seconds{job=\"github-actions\"}",
            "legendFormat": "Build Time",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "single"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 10,
              "pointSize": 5,
              "lineInterpolation": "smooth"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "green" },
                { "value": 300, "color": "yellow" },
                { "value": 600, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Test Duration",
        "type": "timeseries",
        "gridPos": { "x": 12, "y": 4, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "test_duration_seconds{job=\"github-actions\"}",
            "legendFormat": "Test Time",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "single"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 10,
              "pointSize": 5,
              "lineInterpolation": "smooth"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "green" },
                { "value": 60, "color": "yellow" },
                { "value": 120, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Commits per Build",
        "type": "stat",
        "gridPos": { "x": 0, "y": 12, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "github_commits{job=\"github-actions\"}",
            "legendFormat": "Commits",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "blue" }
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Files Changed",
        "type": "stat",
        "gridPos": { "x": 6, "y": 12, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "github_files_changed{job=\"github-actions\"}",
            "legendFormat": "Files",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "purple" }
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Lines Added",
        "type": "stat",
        "gridPos": { "x": 12, "y": 12, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "github_lines_added{job=\"github-actions\"}",
            "legendFormat": "Lines +",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "Lines Removed",
        "type": "stat",
        "gridPos": { "x": 18, "y": 12, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "github_lines_removed{job=\"github-actions\"}",
            "legendFormat": "Lines -",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "title": "Build & Test Trend",
        "type": "timeseries",
        "gridPos": { "x": 0, "y": 16, "w": 24, "h": 8 },
        "targets": [
          {
            "expr": "build_status{job=\"github-actions\"}",
            "legendFormat": "Build Status",
            "refId": "A"
          },
          {
            "expr": "test_status{job=\"github-actions\"}",
            "legendFormat": "Test Status",
            "refId": "B"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 20,
              "pointSize": 5,
              "lineInterpolation": "stepAfter"
            },
            "mappings": [
              { "type": "value", "value": "1", "text": "Success" },
              { "type": "value", "value": "0", "text": "Failed" }
            ]
          },
          "overrides": [
            {
              "matcher": { "id": "byName", "options": "Build Status" },
              "properties": [
                { "id": "color", "value": { "mode": "fixed", "fixedColor": "blue" } }
              ]
            },
            {
              "matcher": { "id": "byName", "options": "Test Status" },
              "properties": [
                { "id": "color", "value": { "mode": "fixed", "fixedColor": "green" } }
              ]
            }
          ]
        }
      },
      {
        "id": 12,
        "title": "Performance Metrics (Duration Distribution)",
        "type": "timeseries",
        "gridPos": { "x": 0, "y": 24, "w": 24, "h": 8 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, build_duration_seconds{job=\"github-actions\"})",
            "legendFormat": "Build P95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, test_duration_seconds{job=\"github-actions\"})",
            "legendFormat": "Test P95",
            "refId": "B"
          },
          {
            "expr": "avg_over_time(build_duration_seconds{job=\"github-actions\"}[1h])",
            "legendFormat": "Build Avg (1h)",
            "refId": "C"
          },
          {
            "expr": "avg_over_time(test_duration_seconds{job=\"github-actions\"}[1h])",
            "legendFormat": "Test Avg (1h)",
            "refId": "D"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["lastNotNull", "mean", "max"]
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 10,
              "pointSize": 4
            }
          }
        }
      }
    ]
}
